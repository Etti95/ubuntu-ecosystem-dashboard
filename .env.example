# Optional: GitHub personal access token for higher rate limits
# Without this, you're limited to 60 requests/hour
# With this, you get 5000 requests/hour
GITHUB_TOKEN=

# Required for Vercel Cron protection
# Generate a random string: openssl rand -hex 32
CRON_SECRET=

# Vercel KV credentials (auto-populated by Vercel when KV is linked)
# For local development, you can get these from Vercel Dashboard
KV_REST_API_URL=
KV_REST_API_TOKEN=
KV_REST_API_READ_ONLY_TOKEN=
KV_URL=
